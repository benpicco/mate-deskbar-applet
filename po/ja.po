msgid "An all-in-one search bar."
msgstr "デスクトップに統合した検索バーです。"

#: ../deskbar/about.py:32
msgid "Deskbar Applet Website"
msgstr "Deskbar アプレットのウェブサイト"

#: ../deskbar/handlers/epiphany.py:12
msgid "Epiphany"
msgstr "Epiphany"

#: ../deskbar/handlers/epiphany.py:12 ../deskbar/handlers/mozilla.py:19
msgid "Index your bookmarks and search engines"
msgstr "ブックマークの内容を検索します"

#: ../deskbar/handlers/files.py:14
msgid "Files"
msgstr "ファイル"

#: ../deskbar/handlers/files.py:14
msgid "Open files by typing their names."
msgstr "ファイル名を入力しながら検索します"

#: ../deskbar/handlers/files.py:37
#, python-format
msgid "Open <b>%(name)s</b>"
msgstr "<b>%(name)s</b> を開く"

#: ../deskbar/handlers/directories.py:14
msgid "Folders"
msgstr "フォルダ"

#: ../deskbar/handlers/directories.py:14
msgid "Open folders by their names."
msgstr "フォルダ名を入力しながら検索します"

#: ../deskbar/handlers/directories.py:29
#, python-format
msgid "Open folder <b>%(name)s</b>"
msgstr "フォルダ <b>%(name)s</b> を開く"

#: ../deskbar/handlers/galago.py:27
#, python-format
msgid "Send Email to <b>%(name)s</b>"
msgstr "<b>%(name)s</b> へ E-メールを送信する"

#: ../deskbar/handlers/gtkbookmarks.py:9
msgid "Nautilus Places"
msgstr "Nautilus で開く"

#: ../deskbar/handlers/gtkbookmarks.py:9
msgid "Open your nautilus favorite places by name."
msgstr "指定した場所を Nautilus で開きます"

#: ../deskbar/handlers/gtkbookmarks.py:23
#, python-format
msgid "Open location <b>%(name)s</b>"
msgstr "場所 <b>%(name)s</b> を開く"

#: ../deskbar/handlers/mozilla.py:19
msgid "Mozilla/Firefox"
msgstr "Mozilla/Firefox"

#: ../deskbar/handlers/programs.py:17
msgid "Desktop Programs"
msgstr "デスクトップ・アプリ"

#: ../deskbar/handlers/programs.py:17
msgid "Index the program names and descriptions."
msgstr "アプリの名前とその概要を検索します"

#: ../deskbar/handlers/programs.py:23
#, python-format
msgid "Search for file names like <b>%(arg)s</b>"
msgstr "<b>%(arg)s</b> に似たファイル名を検索する"

#: ../deskbar/handlers/programs.py:26
#, python-format
msgid "Search <b>%(arg)s</b> in Gnome Dictionary"
msgstr "GNOME ディレクトリから <b>%(arg)s</b> を検索する"

#: ../deskbar/handlers/programs.py:28
#, python-format
msgid "Search <b>%(arg)s</b> with Beagle"
msgstr "Beagle で <b>%(arg)s</b> を検索する"

#: ../deskbar/handlers/programs.py:66
#, python-format
msgid "Launch <b>%(name)s</b> (%(prog)s)"
msgstr "<b>%(name)s</b> (%(prog)s) を起動する"

#: ../deskbar/handlers/pathprograms.py:11
msgid "Command line programs"
msgstr "コマンド・ライン"

#: ../deskbar/handlers/pathprograms.py:11
msgid "Allow to launch any program present in your $PATH"
msgstr "環境変数 $PATH からプログラムを検索して起動します"

#: ../deskbar/handlers/pathprograms.py:33
#, python-format
msgid "Execute <b>%(text)s</b>"
msgstr "<b>%(text)s</b> を起動する"
