#: ../deskbar/handlers/beagle-live.py:46
#, python-format
msgid "Addressbook entry for %s"
msgstr "Položka adresára pre %s"

#. translators: First %s is mail sender, second %s is mail subject.
#: ../deskbar/handlers/beagle-live.py:55
#, python-format
msgid "View email from %s: %s"
msgstr "Zobraziť elektronickú poštu od %s: %s"

#: ../deskbar/handlers/beagle-live.py:68
#, python-format
msgid "Open news item %s"
msgstr "Otovirť položku noviniek %s"

#: ../deskbar/handlers/beagle-live.py:75
#, python-format
msgid "Open note %s"
msgstr "Otvoriť poznámku %s"

#: ../deskbar/handlers/beagle-live.py:81
#, python-format
msgid "View conversation with %s"
msgstr "Zobraziť rozhovor s %s"

#: ../deskbar/handlers/beagle-live.py:87
#, python-format
msgid "View calendar %s"
msgstr "Zobraziť kalendár %s"

