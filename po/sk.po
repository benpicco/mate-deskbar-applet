msgid "Web Addresses"
msgstr "WWW adresy"

#: ../deskbar/handlers/web_address.py:10
msgid "Open webpages by typing an address."
msgstr "Otvoriť WWW stránky napísaním adresy."

#: ../deskbar/handlers/web_address.py:35
#, python-format
msgid "Open the web page %s"
msgstr "Otvoriť www stránku %s"

#: ../deskbar/handlers/web_address.py:37
#, python-format
msgid "Open the location %s"
msgstr "Otvoriť umiestnenie %s"

