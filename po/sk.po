msgid "Use Beagle to search for documents"
msgstr "Použiť Beagle pre vyhľadanie dokumentov"

#: ../deskbar/handlers/beagle.py:33
#, python-format
msgid "Search for %s using Beagle"
msgstr "Vyhľadanie %s pomocou Beagle"

#: ../deskbar/handlers/email_address.py:9
msgid "Email Addresses"
msgstr "Adresa elektronicke pošty"

#: ../deskbar/handlers/email_address.py:10
msgid "Send mails by typing an email address."
msgstr "Odoslať správy napísaním adresy elektronickej pošty."

#: ../deskbar/handlers/galeon.py:27
msgid "Index your history"
msgstr "Indexovať vašu históriu"

#: ../deskbar/handlers/volumes.py:16
msgid "Search for disks and network places"
msgstr "Vyľadať disky a sieťové miesta"

#: ../deskbar/handlers/volumes.py:17
msgid "Open drives and volumes by their name"
msgstr "Otvoriť jednotky a zväzky podľa ich názvov"

#: ../deskbar/handlers/volumes.py:41
#, python-format
msgid "Open network place %s"
msgstr "Otvoriť sieťové miesto %s"

#: ../deskbar/handlers/volumes.py:43
#, python-format
msgid "Open audio disk %s"
msgstr "Otvoriť zvukový disk %s"

