<gconfschemafile>
	<schemalist>
		<schema>
			<key>/schemas/apps/deskbar/expand</key>
			<applyto>/apps/deskbar/expand</applyto>
			<owner>deskbar</owner>
			<type>bool</type>
			<default>FALSE</default>
			<locale name="C">
				<short>Wether the applet should take all the available space in the panel. Overrides the width preference.</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/width</key>
			<applyto>/apps/deskbar/width</applyto>
			<owner>deskbar</owner>
			<type>int</type>
			<default>150</default>
			<locale name="C">
				<short>The applet entry width in pixels</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/keybinding</key>
			<applyto>/apps/deskbar/keybinding</applyto>
			<owner>deskbar</owner>
			<type>string</type>
			<default>&lt;Alt&gt;F3</default>
			<locale name="C">
				<short>The key sequence will focus the deskbar applet, allowing to type quickly</short>
				<!-- <long></long> -->
			</locale>
		</schema>
	</schemalist>
</gconfschemafile>
