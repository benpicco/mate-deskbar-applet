<gconfschemafile>
	<schemalist>
		<schema>
			<key>/schemas/apps/deskbar/expand</key>
			<applyto>/apps/deskbar/expand</applyto>
			<owner>deskbar</owner>
			<type>bool</type>
			<default>FALSE</default>
			<locale name="C">
				<short>Wether the applet should take all the available space in the panel. Overrides the width preference.</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/width</key>
			<applyto>/apps/deskbar/width</applyto>
			<owner>deskbar</owner>
			<type>int</type>
			<default>20</default>
			<locale name="C">
				<short>The applet's entry width in number of characters</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/keybinding</key>
			<applyto>/apps/deskbar/keybinding</applyto>
			<owner>deskbar</owner>
			<type>string</type>
			<default>&lt;Alt&gt;F3</default>
			<locale name="C">
				<short>The key sequence will focus the deskbar applet, allowing to type quickly</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/ui_name</key>
			<applyto>/apps/deskbar/ui_name</applyto>
			<owner>deskbar</owner>
			<type>string</type>
			<default>Entriac</default>
			<locale name="C">
				<short>Selects the user interface to use, one of "Cuemiac", "Entriac" or "Completion"</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/enabled_handlers</key>
			<applyto>/apps/deskbar/enabled_handlers</applyto>
			<owner>deskbar</owner>
			<type>list</type>
        	<list_type>string</list_type>
        	<default>[BeagleHandler,FolderHandler,EmailAddressHandler,FileHandler,GaleonHandler,GtkBookmarkHandler,MozillaHandler,PathProgramsHandler,EpiphanyHandler,ProgramsHandler,VolumeHandler,WebAddressHandler]</default>
			<locale name="C">
				<short>The list of exported class names of the enabled handlers sorted by priority. Leftmost has highest priority</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/collapsed_cat</key>
			<applyto>/apps/deskbar/collapsed_cat</applyto>
			<owner>deskbar</owner>
			<type>list</type>
        	<list_type>string</list_type>
        	<default>[]</default>
			<locale name="C">
				<short>The list of categories to be collapsed when displayed in the 'Cuemiac' UI. Valid category names are defined in Categories.py</short>
				<!-- <long></long> -->
			</locale>
		</schema>
	</schemalist>
</gconfschemafile>
