<gconfschemafile>
	<schemalist>
		<schema>
			<key>/schemas/apps/deskbar/expand</key>
			<applyto>/apps/deskbar/expand</applyto>
			<owner>deskbar</owner>
			<type>bool</type>
			<default>FALSE</default>
			<locale name="C">
				<short>Whether the applet should take all the available space in the panel. Overrides the width preference.</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/width</key>
			<applyto>/apps/deskbar/width</applyto>
			<owner>deskbar</owner>
			<type>int</type>
			<default>20</default>
			<locale name="C">
				<short>The applet's entry width in number of characters</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/minchars</key>
			<applyto>/apps/deskbar/minchars</applyto>
			<owner>deskbar</owner>
			<type>int</type>
			<default>1</default>
			<locale name="C">
				<short>The minimum number of characters that need to be typed before the applet starts showing matches</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/typingdelay</key>
			<applyto>/apps/deskbar/typingdelay</applyto>
			<owner>deskbar</owner>
			<type>int</type>
			<default>250</default>
			<locale name="C">
				<short>The time in miliseconds between a keystroke in the search entry and the actual search being performed</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/keybinding</key>
			<applyto>/apps/deskbar/keybinding</applyto>
			<owner>deskbar</owner>
			<type>string</type>
			<default>&lt;Alt&gt;F3</default>
			<locale name="C">
				<short>The key sequence will focus the deskbar applet, allowing to type quickly</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/ui_name</key>
			<applyto>/apps/deskbar/ui_name</applyto>
			<owner>deskbar</owner>
			<type>string</type>
			<default>Entriac</default>
			<locale name="C">
				<short>Selects the user interface to use, one of "Cuemiac" or "Entriac"</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/enabled_handlers</key>
			<applyto>/apps/deskbar/enabled_handlers</applyto>
			<owner>deskbar</owner>
			<type>list</type>
        	<list_type>string</list_type>
        	<default>[HistoryHandler,ProgramsHandler,PathProgramsHandler,EvolutionHandler,EmailAddressHandler,WebAddressHandler,FileFolderHandler,GtkBookmarkHandler,VolumeHandler,EpiphanyBookmarksHandler,EpiphanyHistoryHandler,EpiphanySearchHandler,GaleonBookmarksHandler,GaleonHistoryHandler,GaleonSearchHandler,MozillaBookmarksHandler,MozillaSearchHandler]</default>
			<locale name="C">
				<short>The list of exported class names of the enabled handlers sorted by priority. Leftmost has highest priority</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/collapsed_cat</key>
			<applyto>/apps/deskbar/collapsed_cat</applyto>
			<owner>deskbar</owner>
			<type>list</type>
        	<list_type>string</list_type>
        	<default>[]</default>
			<locale name="C">
				<short>The list of categories to be collapsed when displayed in the 'Cuemiac' UI. Valid category names are defined in Categories.py</short>
				<!-- <long></long> -->
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/deskbar/mozilla/show_only_primary_search</key>
			<applyto>/apps/deskbar/mozilla/show_only_primary_search</applyto>
			<owner>deskbar</owner>
			<type>bool</type>
			<default>FALSE</default>
			<locale name="C">
				<short>Whether to show only the preferred search engine, rather than all available engines.  This only affects Mozilla-based web browsers such as Firefox.</short>
				<!-- <long></long> -->
			</locale>
		</schema>
	</schemalist>
</gconfschemafile>
